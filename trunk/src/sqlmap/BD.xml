<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="BD">

  <select id="BDMA010CNTQ" parameterType="string" resultType="int">
    select count(*) as CNT
  from brd a

  </select>
  
  <select id="BDMA010Q" parameterType="java.util.HashMap" resultType="java.util.HashMap">
    select a.brd_seq_no,
       a.brd_type,
       a.brd_title,
       a.brd_hit,
       a.content,
       a.reg_id,
       a.reg_nm,
       a.reg_dt,
       a.udt_id,
       a.udt_nm,
       a.udt_dt
  from brd a
	
  </select>


</mapper>